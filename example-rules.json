    {
        "match": "/^https?:\\/\\/(www\\.)?youtube\\.com\\/watch\\?/i",
        "match_type": "href",
        "set_query_parameter": {
            "html5": "1",
            "autoplay": "0"
        }
    },
    {
        "@comment": "Test on http://pgl.yoyo.org/http/browser-headers.php",

        "match": "pgl.yoyo.org",
        "match_type": "hostname",
        "set_request_header": {
            "X-Test-Param": "Bubble"
        }
    },
    {
        "@comment": "Test on http://www.reliply.org/tools/requestheaders.php",

        "match": "www.reliply.org",
        "match_type": "hostname",
        "set_request_header": {
            "User-Agent": "Bubble/1.0"
        }
    },
    {
        "match": "www.some-site-i-want-cross-origin-scripts-to-load-on.tld",
        "match_type": "hostname",
        "set_response_header": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "POST,GET"
        }
    },
    {
        "match": "kafene.org",
        "match_type": "hostname",
        "set_query_parameter": {
            "working": "yes"
        }
    }
