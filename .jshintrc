// http://www.jshint.com/docs/options/
{
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "strict": true,
    "maxdepth": 4,
    "maxstatements": 12,
    "maxlen": 100,
    "eqnull": true,
    "devel": true,
    "sub": true,

    "moz": true
}
